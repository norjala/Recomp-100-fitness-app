{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx --experimental-specifier-resolution=node server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --build tsconfig.server.json --force",
    "start": "NODE_ENV=production node scripts/render-start.mjs",
    "start:railway": "node scripts/railway-start.cjs",
    "clean": "rm -rf dist node_modules .vite",
    "clean:build": "rm -rf dist .vite",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "npm run clean && npm install",
    "rebuild": "npm run clean:build && npm run build",
    "build:analyze": "npm run build && echo '=== Build Analysis ===' && ls -la dist/public/assets/ && echo '=== Forms Chunk Check ===' && ls dist/public/assets/forms-* 2>/dev/null || echo 'WARNING: No forms chunk found!'",
    "preview": "npm run build && npm run start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest --config jest.config.js",
    "test:frontend": "vitest run --config vitest.config.ts",
    "test:watch": "npm-run-all --parallel test:backend:watch test:frontend:watch",
    "test:backend:watch": "jest --config jest.config.js --watch",
    "test:frontend:watch": "vitest --config vitest.config.ts",
    "test:coverage": "npm run test:backend:coverage && npm run test:frontend:coverage",
    "test:backend:coverage": "jest --config jest.config.js --coverage",
    "test:frontend:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:init": "tsx --experimental-specifier-resolution=node -e \"import('./server/db.js').then(m => m.initializeDatabase())\"",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:pg:generate": "drizzle-kit generate --config drizzle.config.postgres.ts",
    "db:pg:migrate": "drizzle-kit migrate --config drizzle.config.postgres.ts",
    "db:pg:push": "drizzle-kit push --config drizzle.config.postgres.ts",
    "db:pg:studio": "drizzle-kit studio --config drizzle.config.postgres.ts",
    "db:backup": "tsx --experimental-specifier-resolution=node scripts/backup-db.ts",
    "db:restore": "tsx --experimental-specifier-resolution=node scripts/restore-db.ts",
    "seed:test-users": "tsx --experimental-specifier-resolution=node scripts/seed-test-users.ts",
    "seed:cleanup": "tsx --experimental-specifier-resolution=node scripts/seed-test-users.ts --cleanup",
    "seed:reset": "tsx --experimental-specifier-resolution=node scripts/seed-test-users.ts --reset",
    "scores:recalculate": "tsx --experimental-specifier-resolution=node scripts/recalculate-scores.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@libsql/client": "^0.15.14",
    "@libsql/sqlite3": "^0.3.1",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.6",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-query": "^5.60.5",
    "@types/multer": "^2.0.0",
    "@ungap/with-resolvers": "^0.1.0",
    "@uppy/aws-s3": "^4.3.2",
    "@uppy/core": "^4.5.2",
    "@uppy/dashboard": "^4.4.2",
    "@uppy/drag-drop": "^4.2.2",
    "@uppy/file-input": "^4.2.2",
    "@uppy/progress-bar": "^4.3.2",
    "@uppy/react": "^4.5.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "lucide-react": "^0.453.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "openai": "^5.13.1",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^3.2.0",
    "pdfjs-dist": "^5.4.54",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "recharts": "^2.15.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.3.5",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0",
    "vite": "^5.4.19",
    "typescript": "5.6.3",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.3",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.11",
    "@types/pdf-parse": "^1.1.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.3.11",
