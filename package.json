    "drizzle-kit": "^0.30.4",
    "esbuild": "^0.25.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.55.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.19.1",
    "vitest": "^1.0.1"
  }
}
